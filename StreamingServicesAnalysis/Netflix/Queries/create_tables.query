DROP TABLE IF EXISTS netflix_catalog;

CREATE TABLE netflix_catalog (
    show_id VARCHAR(10) PRIMARY KEY,
    type VARCHAR(20),
    title VARCHAR(255),
    director VARCHAR(255),
    cast TEXT,
    country VARCHAR(255),
    date_added DATE,
    release_year INT,
    rating VARCHAR(10),
    duration VARCHAR(10),
    listed_in TEXT,
    description TEXT
);

DESCRIBE netflix_catalog;